{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "id": {
      "type": "string",
      "format": "uuid"
    },
    "invoice_id": {
      "type": "string"
    },
    "order_id": {
      "type": "string"
    },
    "code": {
      "type": "integer"
    },
    "created_at": {
      "type": "string",
      "format": "date-time"
    },
    "started_at": {
      "type": "string",
      "format": "date-time"
    },
    "event_status": {
      "type": "string",
      "enum": ["awaiting_user_pickup", "picked_up", "delivered", "cancelled"]
    },
    "event_type": {
      "type": "string",
      "enum": ["delivery", "return"]
    },
    "refunded_amount": {
      "type": "number",
      "minimum": 0
    },
    "id_user": {
      "type": "string",
      "format": "uuid"
    },
    "id_device": {
      "type": "string",
      "format": "uuid"
    },
    "device": {
      "type": "object",
      "properties": {
        "id": {
          "type": "string",
          "format": "uuid"
        },
        "created_at": {
          "type": "string",
          "format": "date-time"
        },
        "name": {
          "type": "string"
        },
        "locker_number": {
          "type": "integer"
        },
        "mode": {
          "type": "string",
          "enum": ["delivery", "pickup"]
        },
        "shared": {
          "type": "boolean"
        },
        "require_image": {
          "type": "boolean"
        },
        "status": {
          "type": "string",
          "enum": ["available", "reserved", "unavailable"]
        },
        "hardware_type": {
          "type": "string"
        },
        "mac_address": {
          "type": "string",
          "pattern": "^([A-Fa-f0-9]{2}:){5}[A-Fa-f0-9]{2}$"
        },
        "price_required": {
          "type": "boolean"
        },
        "id_location": {
          "type": "string",
          "format": "uuid"
        },
        "id_size": {
          "type": "string",
          "format": "uuid"
        },
        "location": {
          "type": "object",
          "properties": {
            "id": {
              "type": "string",
              "format": "uuid"
            },
            "created_at": {
              "type": "string",
              "format": "date-time"
            },
            "name": {
              "type": "string"
            },
            "address": {
              "type": "string"
            },
            "hidden": {
              "type": "boolean"
            },
            "shared": {
              "type": "boolean"
            },
            "latitude": {
              "type": "number"
            },
            "longitude": {
              "type": "number"
            },
            "contact_email": {
              "type": "string",
              "format": "email"
            },
            "contact_phone": {
              "type": "string",
              "pattern": "^\\+\\d{11,15}$"
            },
            "restrict_by_user_code": {
              "type": "boolean"
            },
            "verify_pin_code": {
              "type": "boolean"
            },
            "verify_qr_code": {
              "type": "boolean"
            },
            "verify_url": {
              "type": "boolean"
            },
            "email": {
              "type": "boolean"
            },
            "phone": {
              "type": "boolean"
            },
            "available_devices": {
              "type": "integer"
            },
            "reserved_devices": {
              "type": "integer"
            },
            "maintenance_devices": {
              "type": "integer"
            }
          },
          "required": ["id", "created_at", "name", "address", "latitude", "longitude"]
        },
        "size": {
          "type": "object",
          "properties": {
            "id": {
              "type": "string",
              "format": "uuid"
            },
            "created_at": {
              "type": "string",
              "format": "date-time"
            },
            "name": {
              "type": "string"
            },
            "description": {
              "type": "string"
            },
            "width": {
              "type": "integer"
            },
            "depth": {
              "type": "integer"
            },
            "height": {
              "type": "integer"
            }
          },
          "required": ["id", "created_at", "name", "width", "depth", "height"]
        },
        "lock_status": {
          "type": "string",
          "enum": ["unknown", "locked", "unlocked"]
        },
        "prices": {
          "type": "array",
          "items": {}
        }
      },
      "required": ["id", "created_at", "name", "locker_number", "mode", "shared", "require_image", "status", "hardware_type", "mac_address", "price_required", "id_location", "id_size", "location", "size", "lock_status", "prices"]
    },
    "user": {
      "type": "object",
      "properties": {
        "id": {
          "type": "string",
          "format": "uuid"
        },
        "created_at": {
          "type": "string",
          "format": "date-time"
        },
        "name": {
          "type": "string"
        },
        "phone_number": {
          "type": "string",
          "pattern": "^\\d{10,15}$"
        },
        "email": {
          "type": "string",
          "format": "email"
        },
        "active": {
          "type": "boolean"
        },
        "user_id": {
          "type": "string"
        },
        "pin_code": {
          "type": "string",
          "pattern": "^\\d{4}$"
        },
        "access_code": {
          "type": "string",
          "pattern": "^\\d{6}$"
        },
        "address": {
          "type": "string"
        },
        "require_auth": {
          "type": "boolean"
        },
        "groups": {
          "type": "array",
          "items": {}
        }
      },
      "required": ["id", "created_at", "name", "phone_number", "email", "active", "user_id", "pin_code", "access_code"]
    }
  },
  "required": ["id", "invoice_id", "order_id", "code", "created_at", "started_at", "event_status", "event_type", "refunded_amount", "id_user", "id_device", "device", "user"]
}
